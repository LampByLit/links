{
  "name": "lynx",
  "version": "1.0.0",
  "description": "Customizable Link Preview Generator for Twitter Cards",
  "main": "dist/app.js",
  "scripts": {
    "start": "node --optimize-for-size --max-old-space-size=460 dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "init-db": "node scripts/init-db.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "@prisma/client": "^6.16.2",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.4.0",
    "tailwindcss": "^3.3.3",
    "@mui/material": "^5.14.5",
    "@mui/icons-material": "^5.14.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/multer": "^1.4.7",
    "@types/compression": "^1.7.2",
    "@types/node": "^20.4.5",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "@types/jest": "^29.5.3",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12",
    "prisma": "^6.16.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "prettier": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27"
  },
  "keywords": ["twitter", "cards", "link-preview", "social-media"],
  "author": "LampByLit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LampByLit/lynx"
